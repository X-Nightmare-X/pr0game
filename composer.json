    "ext-session": "*",
    "ext-spl": "*",
    "ext-zlib": "*",
    "dragonmantank/cron-expression": "^3.3",
        "erusev/parsedown": "1.6.0",
    "phpmailer/phpmailer": "v6.5.3",
    "smarty/smarty": "v4.2.1",
    "symfony/var-dumper": "v6.2.3"
